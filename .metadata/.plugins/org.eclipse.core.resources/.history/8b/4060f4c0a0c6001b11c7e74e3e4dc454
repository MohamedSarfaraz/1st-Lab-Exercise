package lcm;

public class FindLCM {
public static void main(String[] args) {
	int number1 = 72, number2 = 140, lcm1;
	lcm1=(number1>number2)?number1:number2;
	while(true)
	{
		if( lcm1 % number1 == 0 && lcm1 % number2 == 0 ) {
			System.out.printf("LCM of %d and %d is %d",number1,number2,lcm1);
		break;
		}
	}
	++lcm1;
}
}
